{
  "name": "music-theory-questions",
  "version": "0.0.3",
  "description": "music theory question/answer generator",
  "main": "lib/index.js",
  "dependencies": {
    "chance": "^1.0.13",
    "teoria": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "mocha": "^3.2.0",
    "npm-watch": "^0.1.9",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "watch": "npm-watch",
    "build": "./build.sh",
    "bundle": "webpack --progress --colors",
    "prepublish": "./build.sh"
  },
  "author": "Greg Pascale (greg.pascale@gmail.com)",
  "license": "ISC",
  "watch": {
    "test": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": [
        "*"
      ]
    },
    "build": {
      "patterns": [
        "src",
        "test",
        "package.json"
      ],
      "extensions": [
        "*"
      ]
    },
    "bundle": {
      "patterns": [
        "src",
        "test",
        "package.json"
      ],
      "extensions": [
        "*"
      ]
    }
  }
}
